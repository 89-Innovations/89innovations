---
import Dynamic from '@/components/Dynamic.astro';
import Hero from '@/components/hero.astro';
import MainLayout from '@/layouts/MainLayout.astro';
import { fetchStrapiData } from '@/queries/strapiQueries';

const data = await fetchStrapiData('/home-page');
const { title, description, hero, about, services, cta, dynamicPageSection } =
  data;
---

<MainLayout title={`89Innovations | ${title}`} description={description}>
  <Hero hero={hero} />
  <Dynamic {dynamicPageSection} />
</MainLayout>
